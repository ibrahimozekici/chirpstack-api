  "paths": {
    "/api/data": {
      "get": {
        "summary": "GetData api method to get data",
        "operationId": "GetData",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/apiListDataResponse"
            }
          }
        },
        "parameters": [
          {
            "name": "zoneID",
            "description": "Service-profile ID to filter on (string formatted UUID).",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "devEUI",
            "description": "Search on name or DevEUI.",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "DeviceTypeID",
            "description": "device_type_id device type id filter.",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "limit",
            "description": "Max number of devices to return in the result-set.",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          },
          {
            "name": "startTime",
            "in": "query",
            "required": false,
            "type": "string"
          },
          {
            "name": "endTime",
            "in": "query",
            "required": false,
            "type": "string"
          }
        ],
        "tags": [
          "DataService"
        ]
      }
    },
    "/api/dataList": {
      "get": {
        "operationId": "GetDataList",
